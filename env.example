# Configuration Docker - Orüme
# Copiez ce fichier en .env et modifiez les valeurs selon vos besoins

# Base de données
DB_HOST=db
DB_USER=orume_user
DB_PASS=orume_password
DB_NAME=orume

# MySQL Root
MYSQL_ROOT_PASSWORD=root_password

# Ports
WEB_PORT=8080          # Port pour le frontend public
ADMIN_PORT=8081        # Port pour l'admin
DB_PORT=3306           # Port pour MySQL
PHPMYADMIN_PORT=8082   # Port pour phpMyAdmin

